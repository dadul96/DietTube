{
	"manifest_version": 2,
	"name": "DietTube",
	"version": "1.0.0",
	"description": "Prevents you from binge-watching recommended YouTube videos.",
	"developer": {
	  "name": "dadul96",
	  "url": "https://github.com/dadul96/DietTube"
	},
	"icons": {
		"48": "icon_48.png",
		"96": "icon_96.png"
	},
	"permissions": [],
	"content_scripts": [
		{
			"matches": ["*://www.youtube.com/*"],
			"css": ["diettube.css"],
			"run_at" : "document_start"
		}
	]
}
